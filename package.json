{
  "name": "gls-eva-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "npm run openapi_network_gen && npm run openapi_user_gen && npm run openapi_administrative_gen",
    "build": "ng build",
    "build_development": "ng build --configuration development",
    "build_stage": "ng build --configuration stage",
    "build_quality": "ng build --configuration quality",
    "build_prod": "ng build --configuration prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --browsers=ChromeHeadless",
    "test_local": "ng test",
    "test_coverage": "ng test --code-coverage --browsers=ChromeHeadless --code-coverage-exclude='src/app/api/**'",
    "test_coverage_local": "ng test --no-watch --code-coverage --code-coverage-exclude='src/app/api/**'",
    "openapi_network_gen": "ng-openapi-gen -c swagger-generation/gls-network-swagger-prop.json",
    "openapi_user_gen": "ng-openapi-gen -c swagger-generation/gls-user-swagger-prop.json",
    "openapi_administrative_gen": "ng-openapi-gen -c swagger-generation/gls-administrative-swagger-prop.json",
    "lint": "ng lint",
    "test_xml": "ng test --no-watch --browsers=ChromeHeadless",
    "docs:generate": "npx compodoc -c compodoc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@azure/msal-angular": "4.0.10",
    "@azure/msal-browser": "4.11.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@popperjs/core": "^2.11.8",
    "@swimlane/ngx-datatable": "^21.0.0-alpha.1",
    "angular-calendar": "^0.31.1",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/localize": "^18.2.0",
    "@compodoc/compodoc": "^1.1.26",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.1.0",
    "eslint": "^9.20.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-openapi-gen": "^0.52.0",
    "prettier": "^3.5.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.23.0"
  }
}
